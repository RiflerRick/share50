<% include initials %>
<title id="titleValue"><%= page %></title>
</head>
<!-- one important factor here is that you cannot -->
<% include header %>

<body>

  <div class="panel panel-default" id="dynComponent">
    <div class="panel-body">

      <!-- <div id="tourAlert" class="alert alert-warning" role="alert" style="visibility:hidden;"></div> -->
      <!-- <div id="partyAlert" class="alert alert-warning" role="alert" style="visibility:hidden;"></div> -->
      <div class="well" id="wellContent" style="display:block;">
        <!-- <button id="wellContentButton" >Click me</button> -->
        <div style="display:<%= displayTour %>">
          <h4 style="margin-left:auto;margin-right:auto;">Visiting place</h4>
          <h2 style="margin-left:auto;margin-right:auto;"><%= destination %></h2>
          <h4>Journey: <%= dateOfJourney %> , Return: <%= dateOfReturn %><h4>
            <h5><%= description %></h5>
            <a role="button" style="margin-left:400px;" href="tourAccepted?uid=<%= uid %>&comId=<%= comId %>&tid=<%= tid %>" class="btn btn-primary">Accept Tour</a>
        </div>
        <div class="alert alert-warning" style="display:<%= displayOver %>">
          <h3 style="margin-left:auto;margin-right:auto;width:500px;">This tour is over!!! Better check out early next time!!!</h3>
        </div>
      <!--  The above technique is the way we include js files into the ejs template-->
    </div>
    <div id="tourDiv" style="display:none;">

    </div>
  </div>
  </div>
  <!-- <script type="text/javascript" src="indexContentGenerator.js"></script> -->
  <!-- <script type="text/javascript" src="indexPage.js"></script> -->
</body>

<% include footer %>
</html>
