  <div class="panel panel-default" id="dynComponent">
    <div class="panel-body" id="content">
      <p id="contentIsFilled"></p>
      <div class="well" id="wellContent">
        <!-- <button id="wellContentButton" >Click me</button> -->
        <form id="tourForm" class="form-horizontal" method="post" action="toursCreate">
  <div class="form-group">
    <label for="destination" class="col-sm-4 control-label">Destination</label>
    <div class="col-sm-8">
      <input id="destination" type="text" name="destination" class="form-control" placeholder="Where would you like to go">
      <!-- name is destination -->
    </div>
  </div>

  <div class="form-group">
    <label for="dateOfJourney" class="col-sm-4 control-label">Leaving on</label>
    <div class="col-sm-8">
      <div class='input-group date' id="journey">
                    <input id="dateOfLeaving" onblur="dateOfLeavingValidation()" type='date' name="leaving" id="leave" class="form-control" />
<!--  name is leaving-->
                </div>
    </div>
  </div>

  <div class="form-group">
    <label for="dateOfReturn" class="col-sm-4 control-label">Returning on</label>
    <div class="col-sm-8">
      <div class='input-group date' id="return">
                    <input id="dateOfReturn" onblur="dateOfReturnValidation()" type='date' name="return" id="return" class="form-control" />
<!--  name is returning-->
                </div>
    </div>
  </div>

  <div class="form-group">
    <label for="description" class="col-sm-4 control-label">Say a few words about the trip</label>
    <div class="col-sm-8">
      <input type="datetime" name="description" class="form-control" id="description" placeholder="Feeling excited!!!">
      <!--  name is description-->
    </div>
  </div>


