<% include initials %>
<title id="titleValue"><%= page %></title>
<script src="tours.js" type="text/javascript"></script>
</head>
<!-- one important factor here is that you cannot -->
<% include header %>
<body>
  <div class="panel panel-default" id="dynComponent">
    <div class="panel-body">
      <p id="contentIsFilled"></p>
      <div class="well" id="wellContent">
        <!-- <button id="wellContentButton" >Click me</button> -->
        <form class="form-horizontal">
  <div class="form-group">
    <label for="destination" class="col-sm-4 control-label">Destination</label>
    <div class="col-sm-8">
      <input type="text" class="form-control" id="destination" placeholder="Where would you like to go">
    </div>
  </div>

  <div class="form-group">
    <label for="dateOfJourney" class="col-sm-4 control-label">Leaving on</label>
    <div class="col-sm-8">
      <div class='input-group date' id='journey'>
                    <input type='text' class="form-control" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
    </div>
  </div>

  <div class="form-group">
    <label for="dateOfReturn" class="col-sm-4 control-label">Returning on</label>
    <div class="col-sm-8">
      <div class='input-group date' id='return'>
                    <input type='text' class="form-control" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
    </div>
  </div>

  <div class="form-group">
    <label for="description" class="col-sm-4 control-label">Say a few words about the trip</label>
    <div class="col-sm-8">
      <input type="datetime" class="form-control" id="description" placeholder="Feeling excited!!!">
    </div>
  </div>


  <div class="form-group">
    <label for="description" class="col-sm-4 control-label">Invite Friends</label>
    <div class="col-sm-8">
      <select class="form-control">
        <option>1</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
      </select>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-5 col-sm-10">
      <button type="submit" class="btn btn-info" >Lets go!!!</button>
    </div>
  </div>
</form>

      </div>

      <!--  The above technique is the way we include js files into the ejs template-->
    </div>
  </div>
</body>
<% include footer %>
</html>
